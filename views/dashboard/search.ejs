<div class="container-fluid container-fluid-custom pb-5 mb-5">

    <div class="row">
      <% if(searchResults != '') { %>
        <% searchResults.forEach(function (task, index) { %>
          
          <a href="/dashboard/item/<%= task._id %>">
          <h4><%= task.title %></h4>
          </a>
  
        <% }) %>
      <% } else { %>
        <p>No items found.</p>
      <% } %>
  
    </div>
  
  </div>
  